def rgb_to_color_name(rgb):\
    """Convert RGB to color name - COLORI ESATTI DA ADV CONSULENZA"""\
    if rgb is None:\
        return None\
    \
    if hasattr(rgb, 'rgb'):\
        rgb_str = rgb.rgb\
    elif isinstance(rgb, str):\
        rgb_str = rgb\
    else:\
        return None\
    \
    # Remove alpha channel\
    if len(rgb_str) == 8:\
        rgb_str = rgb_str[2:]\
    \
    rgb_str = rgb_str[:6].upper()\
    \
    # Ignore non-relevant colors (black, white, headers)\
    if rgb_str in ['FFFFFF', '000000', 'EBEEFB', 'A3C4BC']:\
        return None\
    \
    # COLORI CONFERMATI:\
    # Verde (contratto firmato)\
    if rgb_str in ['92D050', '00B050', '00B04F']:\
