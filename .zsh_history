#!/usr/bin/env python3\
"""\
ADV Consulenza Master Processor\
Processes all PDFs and Excel files including color-coded contract states\
"""\
\
import sys\
import json\
import re\
from pathlib import Path\
from datetime import datetime\
\
import pdfplumber\
import pandas as pd\
from openpyxl import load_workbook\
\
\
def extract_all_anagrafica_pdfs(pdf_paths):\
    """Extract anagrafica data from multiple PDFs"""\
    print(f"\nðƒ¿ƒ³ƒ¤ Estrazione anagrafica da {len(pdf_paths)} PDF...")\
    \
    all_agencies = {}\
    \
    for pdf_path in pdf_paths:\
        print(f"  â€ƒ‚ Leggendo {Path(pdf_path).name}...")\
        \
        with pdfplumber.open(pdf_path) as pdf:\
            for page in pdf.pages:\
                text = page.extract_text()\
                if not text:\
                    continue\
                \
                lines = text.split('\n')\
                current_agency = None\
                \
                for line in lines:\
                    code_match = re.search(r'\b([a-z]{2}[a-z0-9]+)\b', line.lower())\
                    if code_match:\
                        code = code_match.group(1).lower()\
                        if code not in all_agencies:\
                            all_agencies[code] = {\
                                'codice': code,\
                                'ragione_sociale': '',\
                                'indirizzo': '',\
                                'telefono': '',\
                                'email': '',\
                                'pec': ''\
                            }\
                        current_agency = code\
                    \
                    if current_agency:\
                        line_lower = line.lower()\
                        \
                        if 'ragione sociale' in line_lower or 'agenzia' in line_lower:\
                            if not all_agencies[current_agency]['ragione_sociale']:\
                                all_agencies[current_agency]['ragione_sociale'] = line.strip()\
                        elif '@' in line and 'pec' not in line_lower:\
                            if not all_agencies[current_agency]['email']:\
                                all_agencies[current_agency]['email'] = line.strip()\
                        elif '@pec' in line_lower or 'pec' in line_lower:\
                            if not all_agencies[current_agency]['pec']:\
                                all_agencies[current_agency]['pec'] = line.strip()\
                        elif re.search(r'\+?39[\s\-]?\d{2,3}[\s\-]?\d{6,8}', line):\
                            if not all_agencies[current_agency]['telefono']:\
                                all_agencies[current_agency]['telefono'] = line.strip()\
                        elif re.search(r'via|corso|piazza|viale', line_lower):\
                            if not all_agencies[current_agency]['indirizzo']:\
                                all_agencies[current_agency]['indirizzo'] = line.strip()\
    \
    print(f"  âƒ¼ƒ¥ Estratte {len(all_agencies)} agenzie")\
    return all_agencies\
\
\
def extract_all_performance_pdfs(pdf_paths):\
    """Extract performance data from multiple PDFs"""\
    print(f"\nðƒ¿ƒ³ƒª Estrazione performance da {len(pdf_paths)} PDF...")\
    \
    all_performance = {}\
    \
    for pdf_path in pdf_paths:\
        print(f"  â€ƒ‚ Leggendo {Path(pdf_path).name}...")\
        \
        with pdfplumber.open(pdf_path) as pdf:\
            for page in pdf.pages:\
                text = page.extract_text()\
                if not text:\
                    continue\
                \
                lines = text.split('\n')\
                current_code = None\
                \
                for line in lines:\
                    code_match = re.search(r'\b([a-z]{2}[a-z0-9]+)\b', line.lower())\
                    if code_match:\
                        current_code = code_match.group(1).lower()\
                        if current_code not in all_performance:\
                            all_performance[current_code] = {\
                                'codice': current_code,\
                                'recensioni_totali': 0,\
                                'valutazione_media': 0.0,\
                                'visualizzazioni_totali': 0,\
                                'chiamate': 0,\
                                'richieste_itinerario': 0,\
                                'visite_sito_web': 0\
                            }\
                    \
                    if current_code:\
                        line_lower = line.lower()\
                        numbers = re.findall(r'\d+(?:\.\d+)?', line)\
                        \
                        if 'recensioni' in line_lower and numbers:\
                            all_performance[current_code]['recensioni_totali'] = int(float(numbers[0]))\
                        elif 'valutazione' in line_lower or 'rating' in line_lower:\
                            if numbers:\
                                all_performance[current_code]['valutazione_media'] = float(numbers[0])\
                        elif 'visualizzazioni' in line_lower and numbers:\
                            all_performance[current_code]['visualizzazioni_totali'] = int(float(numbers[0]))\
                        elif 'chiamate' in line_lower and numbers:\
                            all_performance[current_code]['chiamate'] = int(float(numbers[0]))\
                        elif 'itinerario' in line_lower or 'direzioni' in line_lower:\
                            if numbers:\
                                all_performance[current_code]['richieste_itinerario'] = int(float(numbers[0]))\
                        elif ('sito' in line_lower or 'website' in line_lower) and numbers:\
                            all_performance[current_code]['visite_sito_web'] = int(float(numbers[0]))\
    \
    print(f"  âƒ¼ƒ¥ Estratte performance per {len(all_performance)} agenzie")\
    return all_performance\
\
\
def extract_amministrazione_excel(excel_path):\
    """Extract amministrazione data from Excel"""\
    print(f"\nðƒ¿ƒ²¼ Estrazione dati amministrativi...")\
    \
    df = pd.read_excel(excel_path)\
    amministrazione = {}\
    \
    for _, row in df.iterrows():\
        code = None\
        for col in df.columns:\
            val = str(row[col]).lower()\
            code_match = re.search(r'\b([a-z]{2}[a-z0-9]+)\b', val)\
            if code_match:\
                code = code_match.group(1)\
                break\
        \
        if not code:\
            continue\
        \
        stato = 'Arancio'\
        row_text = ' '.join([str(v).lower() for v in row.values if pd.notna(v)])\
        \
        if 'bonifico' in row_text or 'pagato' in row_text or 'verde' in row_text:\
            stato = 'Verde'\
        elif 'sospeso' in row_text or 'rosso' in row_text:\
            stato = 'Rosso'\
        \
        amministrazione[code] = {\
            'codice': code,\
            'stato': stato,\
            'note': ''\
        }\
    \
    print(f"  âƒ¼ƒ¥ Estratti dati per {len(amministrazione)} agenzie")\
    return amministrazione\
\
\
def rgb_to_color_name(rgb):\
    """Convert RGB to color name"""\
    if rgb is None:\
        return None\
    \
    if hasattr(rgb, 'rgb'):\
        rgb_str = rgb.rgb\
    elif isinstance(rgb, str):\
        rgb_str = rgb\
    else:\
        return None\
    \
    if len(rgb_str) == 8:\
        rgb_str = rgb_str[2:]\
    \
    rgb_str = rgb_str.upper()\
    \
    if 'FF' in rgb_str and ('C0' in rgb_str or 'D0' in rgb_str or 'E0' in rgb_str or 'F0' in rgb_str):\
        if 'FF00' in rgb_str:\
            return 'Giallo'\
        return 'Arancione'\
    \
    if '00' in rgb_str and ('B0' in rgb_str or '90' in rgb_str or 'C0' in rgb_str or 'D0' in rgb_str):\
        return 'Verde'\
    \
    return None\
\
\
def extract_contratti_from_agent_excel(excel_paths):\
    """Extract contract data from agent Excel files with color coding"""\
    print(f"\nðƒ¿ƒ³ƒ½ Estrazione contratti da {len(excel_paths)} Excel agenti...")\
    \
    contratti = {}\
    \
    for excel_path in excel_paths:\
        agent_name = Path(excel_path).stem.replace(' Anagrafica', '')\
        print(f"  â€ƒ‚ Leggendo {agent_name}...")\
        \
        wb = load_workbook(excel_path)\
        ws = wb.active\
        df = pd.read_excel(excel_path)\
        \
        for idx, row in df.iterrows():\
            code = None\
            for col in df.columns:\
                val = str(row[col]).lower()\
                code_match = re.search(r'\b([a-z]{2}[a-z0-9]+)\b', val)\
                if code_match:\
                    code = code_match.group(1)\
                    break\
            \
            if not code:\
                continue\
            \
            excel_row = idx + 2\
            row_color = None\
            \
            for cell_idx in range(1, min(10, ws.max_column + 1)):\
                cell = ws.cell(row=excel_row, column=cell_idx)\
                if cell.fill and cell.fill.start_color:\
                    rgb = cell.fill.start_color.rgb\
                    color_name = rgb_to_color_name(rgb)\
                    if color_name:\
                        row_color = color_name\
                        break\
            \
            stato_contratto = 'In attesa'\
            if row_color == 'Verde':\
                stato_contratto = 'Firmato'\
            elif row_color == 'Arancione':\
                stato_contratto = 'Inviato'\
            elif row_color == 'Giallo':\
                stato_contratto = 'In attesa'\
            \
            contratti[code] = {\
                'codice': code,\
                'stato': stato_contratto,\
                'agente': agent_name\
            }\
    \
    print(f"  âƒ¼ƒ¥ Estratti contratti per {len(contratti)} agenzie")\
    return contratti\
\
\
def merge_all_data(anagrafica, performance, amministrazione, contratti):\
    """Merge all data sources"""\
    print("\nðƒ¿ƒ´ƒ¤ Unione di tutti i dati...")\
    \
    all_codes = set()\
    all_codes.update(anagrafica.keys())\
    all_codes.update(performance.keys())\
    all_codes.update(amministrazione.keys())\
    all_codes.update(contratti.keys())\
    \
    merged = {}\
    timestamp = datetime.now().isoformat()\
    \
    for code in all_codes:\
        merged[code] = {\
            'codice': code,\
            'anagrafica': anagrafica.get(code, {}),\
            'performance': performance.get(code, {}),\
            'amministrazione': amministrazione.get(code, {}),\
            'contratti': contratti.get(code, {}),\
            'timestamp': timestamp\
        }\
    \
    print(f"  âƒ¼ƒ¥ Dati uniti per {len(merged)} agenzie totali")\
    return merged\
\
\
def main():\
    print("=" * 60)\
